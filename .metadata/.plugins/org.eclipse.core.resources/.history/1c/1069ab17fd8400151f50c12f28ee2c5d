Ti.UI.setBackgroundColor("#000");

var navWindow, mainWindow;

//requiring modules
mainWindow = require('mainWindowView').mainWindow;

//define navigation window
navWindow = Ti.UI.iOS.createNavigationWindow({
	window: mainWindow
});

//open navigation window
navWindow.open();